            "types": "./dist/storage/interface.d.ts",
            "import": "./dist/storage/interface.js",
            "require": "./dist/storage/interface.js"
        },
        "./storage/web": {
            "types": "./dist/storage/web.d.ts",
            "import": "./dist/storage/web.js",
            "require": "./dist/storage/web.js"
        },
        "./storage/mobile": {
